  "query": "\n            SELECT \n                auths.id,\n                auths.client_id,\n                auths.user_id,\n                auths.scopes,\n                auths.created,\n                clients.name as client_name,\n                clients.icon_url as client_icon_url,\n                clients.created_by as client_created_by\n            FROM oauth_client_authorizations auths\n            JOIN oauth_clients clients ON clients.id = auths.client_id\n            WHERE user_id=$1\n            ",
